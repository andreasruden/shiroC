//! run

fn f(i: i32, f: i32) -> i32 {  //! warning: "'f' shadows previous declaration"
	return (f + 2) * i;
}

fn main() -> i32 {
	var i: i32;

	i = 3;
	if (i > 5) {
		i = 10;
	}
	else {
		i = 20;
	}
	printI32(i);  //! stdout: "20"

	while (i > 10) {
		i = 10;
	}

	var result = f(10, i);
	printI32(result);  //! stdout: "120"
	return 0;
}
