//! run: "build/basic_project/bin/Main"

import Self.Calculator;
import Std.IO;

// This collides with an export from Self.Calculator
fn add(a: i32, b: i32) -> i32
{
    return a * b;
}

fn main() -> i32
{
    var adder = Adder{};  // Calculator's exported class (same as Self.Calculator.Adder)
    print_int(adder.add(10, 20));  //! stdout: "30"
    print_int(adder.last_result);  //! stdout: "30"

    var i = add(100, 200);  // "buggy" add
    print_int(i);  //! stdout: "20000"

    // access Calculator's exported add function
    i = Self.Calculator.add(100, 200);
    print_int(i);  //! stdout: "300"

    return 0;
}
